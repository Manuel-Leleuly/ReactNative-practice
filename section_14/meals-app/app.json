      "package": "com.manuel_leleuly.myrecipebook"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "31d3d4a9-993c-4ccd-a402-f0194da21e69"
      }
